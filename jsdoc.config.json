{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".js",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "default":{
            "includeDate":false
        }
    },
    "markdown":{
        "idInHeadings":true
    },
    "opts":{
        "destination":"./site/prod",
        "package":"./package.json",
        "readme":"README.md",
        "recurse":true,
        "template":"./docs/site_template"
    }
}
